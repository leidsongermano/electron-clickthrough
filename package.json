{
  "name": "electron-quick-start",
  "description": "electron-quick-start",
  "author": "electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "start-gpu": "electron --enable-transparent-visuals --disable-gpu main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^20.15.3",
    "electron-prebuilt": "^0.36.12"
  },
  "build": {
    "appId": "test",
    "win": {
      "target": "NSIS"
    }
  },
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "postinstall": "electron-builder install-app-deps"
}
